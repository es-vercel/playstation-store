    "@typechain/hardhat": "^6.1.2",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/howler": "^2.2.7",
    "@types/html-to-text": "^8.1.0",
    "@types/jest": "^28.1.7",
    "@types/js-cookie": "^3.0.1",
